    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^14.0.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^4.0.8",
